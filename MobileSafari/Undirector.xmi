{
    libundirect_MSHookMessageEx = MSHookMessageEx;
}

void initDynamic()
{
    void* libundirect_handle = dlopen("/usr/lib/libundirect.dylib", RTLD_NOW);

    libundirect_MSHookMessageEx = (void (*)(__unsafe_unretained Class, SEL, IMP, IMP *))dlsym(libundirect_handle, "libundirect_MSHookMessageEx");
    libundirect_rebind = (void (*)(void *, __unsafe_unretained Class, SEL, const char *))dlsym(libundirect_handle, "libundirect_rebind");
    libundirect_find = (void *(*)(NSString *__strong, unsigned char *, size_t, unsigned char))dlsym(libundirect_handle, "libundirect_find");
    libundirect_failedSelectors = (NSArray *(*)())dlsym(libundirect_handle, "libundirect_failedSelectors");
}

